{
	env: {
		node: true,
		es6: true,
		mocha: true
	},
	extends: ["airbnb-base"],
	parser: "babel-eslint",
	parserOptions: {
		ecmaVersion: 6,
		ecmaFeatures: {,
			experimentalObjectRestSpread: true,
			jsx: true,
		},
		sourceType: "module",
	},
	plugins: ["babel", "import", "vue"],
	rules: {
		no-tabs: 0,
		no-shadow: 0,
		no-param-reassign: 0,
		indent: ["error", "tab"]
	},
}
